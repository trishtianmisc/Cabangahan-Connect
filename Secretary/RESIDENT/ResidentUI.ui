              <property name="styleSheet">
               <string notr="true">/* Base QLineEdit styling */
QLineEdit {
    background-color: #fff;           /* white background */
    border: 1px solid #ccc;           /* light gray border */
    border-radius: 6px;               /* rounded corners */
    padding: 6px 8px;                 /* vertical/horizontal padding */
    font-size: 14px;                  /* sensible text size */
    color: #333;                      /* dark text */
}

/* Hover state: slightly darker border */
QLineEdit:hover {
    border: 1px solid #999;
}

/* Focus state: blue glow and stronger border */
QLineEdit:focus {
    border: 1px solid #409EFF;        /* blue accent */
    outline: none;                    /* remove default focus ring */
}

/* Disabled state: muted look */
QLineEdit:disabled {
    background-color: #f5f5f5;
    border: 1px solid #e0e0e0;
    color: #999;
}

/* Placeholder text styling */
QLineEdit[echoMode=&quot;0&quot;] {            /* only applies when not password mode */
    qproperty-placeholderText: &quot;Enter text…&quot;;
}
QLineEdit::placeholder {
    color: #999;
    font-style: italic;
}
